                                    <input type="checkbox" class="custom-control-input" id="defaultInline2" asp-for="StayingOver">
                                    <label class="custom-control-label" for="defaultInline2">Staying Overseas</label>
                                </small>
                            </div>
                            <div class="col-sm-3 mt-2">
                                <label class="mt-1">Current Address</label>
                            </div>
                            <div class="col-sm-3 mt-2">
                                <select class="custom-select"
