        </div>
        <div class="card shadow-lg">

            <div class="card-body bg-light">
                @foreach (Resume Resumelist in (Resume[])ViewBag.ResumeRegis)
                {
            <div class="tab-content" id="pills-tabContent">
                <div class="tab-pane fade show active" id="pills-Personel" role="tabpanel">
                    <div class="card-header">
                        <h3 class="font-weight-bold text-dark">Personal Information</h3>
                    </div>

                    <div class="card-body row">

                        <div class="col-sm-9 form-group row">
                            <div class="col-sm-3 mt-2">
                                <label class="mt-1">First Name</label>
                            </div>
                            <div class="col-sm-8 mt-2">
                                <input type="text" class="form-control check" asp-for="FirstName" value="@Resumelist.FirstName" placeholder="Typing First Name" required />
                                <span asp-validation-for="FirstName" class="text-danger"></span>
                            </div>

                            <div class="col-sm-3 mt-2">
                                <label class="mt-1">Last Name</label>
                            </div>
                            <div class="col-sm-8 mt-2">

                                <input type="text" class="form-control" asp-for="LastName" value="@Resumelist.LastName" placeholder="Typing Last Name" required />
                            </div>
                            <div class="col-sm-3 mt-2">
                                <label class="mt-1">Gender</label>
                            </div>
                            <div class="col-sm-8 mt-2">
                                <div class="nav nav-pills" id="gender" role="tablist">

                                    <div class="btn-group btn-group-toggle" data-toggle="buttons">
                                        <label class="nav-link border btn">
                                            <input type="radio" value="Male" asp-for="GenderId" checked="@(Resumelist.GenderId == "Male" ? "checked" : null)"> Male
                                        </label>
                                        <label class="nav-link border btn">
                                            <input type="radio" autocomplete="off" value="Female" asp-for="GenderId" checked="@(Resumelist.GenderId == "Female" ? "checked" : null)"> Female
                                        </label>
                                        <label class="nav-link border btn">
                                            <input type="radio" value="Other" asp-for="GenderId" checked="@(Resumelist.GenderId == "Other" ? "checked" : null)"> Other
                                        </label>
                                    </div>

                                </div>
                            </div>
                            <div class="col-sm-3 mt-2">
                                <label class="mt-1">Birth Date</label>
                            </div>
                            <div class="col-sm-2 mt-2">
                                <select class="custom-select" asp-for="DayB" required>
                                    <option value="0">Date</option>
                                    @for (int Day = 1; Day <= 31; Day++)
                                    {

                                <option value="@Day" selected="@(Resumelist.DayB == Day ? "selected" : null)" >@Day</option> }

                                </select>
                            </div>
                            <div class="col-sm-2 mt-2">
                                <select class="custom-select" asp-for="MonthB" required>
                                    <option value="0">Month</option>
                                    @for (int Month = 1; Month <= 12; Month++)
                                    {

                    <option value="@Month" selected="@(Resumelist.MonthB == Month ? "selected" : null)">@Month</option>
}

                                </select>
                            </div>
                            <div class="col-sm-2 mt-2">
                                <select class="custom-select ml-2" asp-for="YearsB" required>
                                    <option value="0">Year</option>
                                    @for (int i = DateTime.Now.Year; i >= (DateTime.Now.Year - 120); i--)
                                    {
                    <option value="@i" selected="@(Resumelist.YearsB == i ? "selected" : null)">@i</option>
}
                                </select>
                            </div>
                            <div class="col-sm-3 mt-2"></div>
                            <div class="col-sm-3 mt-2">
                                <label class="mt-1">Mobile Phone</label>
                            </div>
                            <div class="col-sm-5 mt-2">

                                <input type="tel" class="form-control" placeholder="Typing Mobile No." value="@Resumelist.MobilePhone" asp-for="MobilePhone" required />
                            </div>
                            <div class="col-sm-4 mt-2"></div>
                            <div class="col-sm-3 mt-2"></div>
                            <div class="custom-control custom-checkbox custom-control-inline col-sm-8 mt-2 ml-4">
                                <small>
                                    <input type="checkbox" class="custom-control-input" id="defaultInline1" asp-for="SameMobile">
                                    <label class="custom-control-label" for="defaultInline1">Same with Mobile Phone</label>
                                </small>
                            </div>

                            <div class="col-sm-3 mt-2">
                                <label class="mt-1">Tel No.</label>
                            </div>
                            <div class="col-sm-5 mt-2">
                                <input type="tel" class="form-control" placeholder="Typing Tel No." value="@Resumelist.Tel" asp-for="Tel" />
                            </div>
                            <div class="col-sm-4 mt-2"></div>
                            <div class="col-sm-3 mt-2">
                                <label class="mt-1">Marriage Status</label>
                            </div>
                            <div class="col-sm-3 mt-2">
