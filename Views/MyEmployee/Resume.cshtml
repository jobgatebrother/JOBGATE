                                    <option value="">Province</option>
                                </select>
                            </div>
                            <div class="col-sm-3 mt-2">
                                <select class="custom-select" id="DistrictList" asp-for="DistrictID">
                                    <option value="">District</option>

                                </select>
                            </div>
                            <div class="col-sm-3 mt-2">
                                <select class="custom-select" id="SubDistrictList" asp-for="SubDistrictID" required>
                                    <option value="0">Sub-District</option>
                                </select>
                            </div>
                            <div class="col-sm-3 mt-2"></div>
                            <div class="col-sm-9 mt-2">

                                <input type="text" class="col-sm form-control" placeholder="Address" value="@Resumelist.Address" asp-for="Address" required />
                            </div>
                        </div>
                        <div class="col-sm-3 mt-2">
                            <div class="col-1"></div>

                            <div class="card mt-2 ml-4" style="width:155px; height:197px;">
                                <img id="image_preview" class="card-img w-100 h-100 bg-preset1" src="@(Resumelist.Image != null? "/Employee/Img/"+Resumelist.Image : "~/Images/user.png")"  />
                            </div>
                            <input onchange="readfile(this);" type="file" class="form-control-file mt-2" asp-for="ImageName" accept="image/x-png,image/gif,image/jpeg"/>
                        </div>
                        <div class="col-sm mt-2 row">
                            <div class="col-sm-3 row">
                                <label class="mt-1">Height</label>
                                <input type="text" class="col-sm-4 form-control ml-2 text-center" placeholder="180" value="@Resumelist.Height" asp-for="Height" required />
                                <small class="mt-2 ml-1">CM</small>
                            </div>
                            <div class="col-sm-3 row">
                                <label class="mt-1">Weight</label>
                                <input type="text" class="col-sm-4 form-control ml-2 text-center" placeholder="60" value="@Resumelist.Weight" asp-for="Weight" required />
                                <small class="mt-2 ml-1">KG</small>
                            </div>
                            <div class="col-sm-3 row mr-4">
                                <label class="mt-1">Religion</label>
                                <select asp-for="ReligionId" class="custom-select col-sm-8 ml-2">
                                    <option value="">Select Religion</option>
                                    <option value="Buddhism" selected="@(Resumelist.ReligionId == "Buddhism" ? "selected" : null)">Buddhism</option>
                                    <option value="Jainism" selected="@(Resumelist.ReligionId == "Jainism" ? "selected" : null)">Jainism</option>
                                    <option value="Hinduism" selected="@(Resumelist.ReligionId == "Hinduism" ? "selected" : null)">Hinduism</option>
                                    <option value="Folk religion" selected="@(Resumelist.ReligionId == "Folk religion" ? "selected" : null)">Folk religion</option>
                                    <option value="Taoism and Confucianism" selected="@(Resumelist.ReligionId == "Taoism and Confucianism" ? "selected" : null)">Taoism and Confucianism</option>
                                    <option value="Islam" selected="@(Resumelist.ReligionId == "Islam" ? "selected" : null)"> Islam</option>
                                    <option value="Christianity" selected="@(Resumelist.ReligionId == "Christianity" ? "selected" : null)">Christianity</option>
                                    <option value=" Sikhism" selected="@(Resumelist.ReligionId == "Sikhism" ? "selected" : null)">Sikhism</option>
                                    <option value="Judaism" selected="@(Resumelist.ReligionId == "Judaism" ? "selected" : null)">Judaism</option>

                                </select>
                            </div>
                            <div class="col-sm-3 row ml-3">
                                <label class="mt-1">Military</label>
                                <select asp-for="MilitaryId" class="custom-select col-sm-8 ml-2">
                                    <option value="" >Select Religion</option>
                                    <option value="Pass" selected="@(Resumelist.MilitaryId == "Pass" ? "selected" : null)">Pass</option>
                                    <option value="Excepted" selected="@(Resumelist.MilitaryId == "Excepted" ? "selected" : null)">Excepted</option>
                                    <option value="None" selected="@(Resumelist.MilitaryId == "None" ? "selected" : null)">None</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <hr />

                    <div class="row justify-content-end">
                        <input type="button" id="btnPersonelNext" class="btn btn-primary col-sm-3 StepNext" data-toggle="pill" href="#pills-Education" role="tab" value="Next" />
