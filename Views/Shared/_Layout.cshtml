<body class="d-flex flex-column min-vh-100">
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-WQ2LW2X"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand font-weight-bold" asp-area="" asp-controller="Home" asp-action="Home">
                    <img src="~/Images/Logo 1.png" width="50" height="50" />
                    JOBGATE
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial.cshtml" />
                    <ul class="navbar-nav mr-3">
                        <li class="nav-item">
                            <a class="nav-link text-dark font-weight-bolder" asp-controller="Home" asp-action="JobResult">Search Job</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark font-weight-bolder" asp-controller="Home" asp-action="Resume">Resume</a>
                        </li>
                        <li class="nav-item mr-3">
                            <a class="nav-link text-dark font-weight-bolder" asp-controller="Home" asp-action="JobMatching">Job Matching</a>
                        </li>
                        <li class="nav-item border-right">
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <content>
        <div class="container">
            <main role="main" class="pb-3" >
                @RenderBody()
            </main>
        </div>
    </content>
    <footer class=" mt-auto" style="background-color: #e6ffff;">
        <div class="container-fluid">
            <div class="row" style="font-size:16px; padding:15px;">

                <div class="col-lg-4 col-md-12 col-12">
                    <div class="table">
                        <table class="table table-sm table-borderless">
                            <tr>
                                <td><h5 class="text-uppercase font-weight-bolder text-primary mt-1">JOBGATE</h5></td>
                            </tr>
                            <tr>
                                <td>
                                    24 Hours Customer &nbsp;
                                    Contacts: 02-000-0000 <br />
                                    Advertisement : support@myjobgate.com

                                </td>
                            </tr>

                        </table>

                    </div>
                </div>
                <div class="col-lg-4 col-md-6 col-6">
                    <div class="table">
                        <table class="table table-sm table-borderless">
                            <tr>
                                <td>&nbsp;</td>
                            </tr>
                            <tr>
                                <td>
                                    <a href="@Url.Action("Home","Home")" class="text-primary d-flex">About</a>
                                    <a href="@Url.Action("Home","Home")" class="text-primary d-flex">Advertisement</a>

                                </td>
                            </tr>

                        </table>

                    </div>
                </div>
                <div class="col-lg-4 col-md-6 col-6">
                    <div class="table">
                        <table class="table table-sm table-borderless">
                            <tr>
                                <td>&nbsp;</td>
                            </tr>
                            <tr>
                                <td>
                                    <a href="@Url.Action("Privacy", "Terms")" class="text-dark d-flex">Privacy Policy</a>
                                    <a href="@Url.Action("Cookie", "Terms")" class="text-dark d-flex">Cookie Policy</a>
                                    <a href="@Url.Action("TermsOfService", "Terms")" class="text-dark d-flex">Terms of Service Agreement</a>

                                </td>
                            </tr>

                        </table>

                    </div>
                </div>
            </div>
        </div>
    </footer>

