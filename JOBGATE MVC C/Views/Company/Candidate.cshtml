@{ Layout = "_Layout Company"; }

<section class="mt-md-5">
    <div class="container">
        <div class="card-header bg-white border-0">
            <h3 class="fw-bolder text-primary">Search Candidate</h3>
        </div>
        <div class="row">
            <div class="col-3">
                <div class="card-body shadow bg-light">
                    <h5 class="fw-bolder">Job Field</h5>
                    <select class="form-select">
                        <option>All</option>
                    </select>
                    <h5 class="fw-bolder mt-2">Location</h5>
                    <select class="form-select">
                        <option>All</option>
                    </select>
                    <h5 class="fw-bolder mt-2">Education Degree</h5>
                    <select class="form-select">
                        <option>All</option>
                    </select>
                    <h5 class="fw-bolder mt-4">Keyword</h5>
                    <input type="text" class="form-control" placeholder="Searching Keyword" />
                    <div class="text-center mt-3">
                        <button type="button" class="btn btn-outline-primary col-sm-6 shadow">Search</button>
                    </div>
                </div>
            </div>
            <div class="col-9">
                <div class="card-body shadow bg-white">
                    <div class="row">
                        <div class="col-sm-2">
                            <img src="~/Images/user.png" class="rounded" style="width:100px;height:100px;" />
                            <p class="fw-bolder mt-4 small">#Gender</p>
                            <p class="fw-bolder small">#Age</p>
                        </div>
                        <div class="col-sm-10">
                            <div class="row">
                                <div class="col-2 col-sm row">
                                    <p class="fw-bolder small col-sm-10">#Resume Title Name</p>
                                    <p class="fw-bolder small w-auto text-end"><small>@DateTime.Now.ToString("d-M-yyyy hh:mm")</small></p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-2 col-sm row">
                                    <p class="fw-bolder small w-auto">Preferred Work Location : </p>
                                    <p class="fw-bolder small w-auto">#Work Location of "Required Job page"</p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-2 col-sm row">
                                    <p class="fw-bolder small col-sm-6">#Highest Education Degree</p>
                                    <p class="fw-bolder small col-sm-6">#Education Major Subject</p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-2 col-sm">
                                    <p class="fw-bolder small">#School Name</p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-2 col-sm row">
                                    <p class="fw-bolder small w-auto">Expected Salary : </p>
                                    <p class="fw-bolder small w-auto">#Expected Salary of "Required Job page"</p>
